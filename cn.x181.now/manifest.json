{
  "manifest_version": 2,
  "id": "cn.x181.now",
  "name": "切换搜索引擎",
  "author": "tt",
  "version": "1.0.1.1",
  "description": "不同搜索引擎下快速任意切换, 方便结果对比!",
  "icons": {
    "16": "default.ico",
    "32": "default-big.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "contextMenus"
  ],
  "background": {
    "page": "background.html",
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}