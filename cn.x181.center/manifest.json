{
  "manifest_version": 2,
  "id": "cn.x181.center",
  "name": "居中显示图片/视频",
  "author": "tt",
  "version": "1.1.0",
  "description": "居中显示图片/视频, 方便大显示器....",
  "icons": {
    "16": "default.ico",
    "32": "default-big.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "http://*/*"
  ],
  "background": {
    "page": "background.html",
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}