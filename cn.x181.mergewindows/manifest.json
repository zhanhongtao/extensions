{
  "manifest_version": 2,
  "id": "cn.x181.mergewindows",
  "name": "合并浏览器窗口",
  "author": "tt",
  "version": "1.0.2",
  "description": "把多个浏览器窗口合并为一个",
  "show_icon": true,
  "permissions": [
    "tabs"
  ],
  "background": {
    "page": "background.html",
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  }
}